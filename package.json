{
  "name": "kev",
  "version": "1.2.0",
  "description": "Key-Value API with pluggable backends. So simple, even Kevin Malone could use it.",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint src",
    "performance": "node src/__tests__/performance.js",
    "precommit": "npm run lint",
    "prepush": "npm run dc:test",
    "dc:destroy": "docker-compose $(make dc.files) down -v --remove-orphans",
    "dc:test": "docker-compose $(make dc.files) build test && docker-compose $(make dc.files) run test test --"
  },
  "keywords": [
    "key",
    "value",
    "key",
    "value",
    "database",
    "storage"
  ],
  "author": "Finbox Inc.",
  "license": "MIT",
  "devDependencies": {
    "delay": "4.3.0",
    "envvar": "2.0.0",
    "eslint": "6.4.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.17.0",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "husky": "3.0.7",
    "jest": "24.9.0",
    "uid": "0.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/finboxio/kev"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "core.spec.js"
    ]
  },
  "dependencies": {
    "@fnbx/mongodb": "3.3.2",
    "bytes": "3.1.0",
    "dataloader": "1.4.0",
    "glob-to-regexp": "0.4.1",
    "into-stream": "5.1.0",
    "ioredis": "4.14.1",
    "lru-cache": "5.1.1",
    "ms": "2.1.2",
    "resurrect-js": "1.0.1",
    "stream-to-array": "2.3.0",
    "stream-transform": "2.0.1"
  }
}
