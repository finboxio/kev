{
  "name": "kev",
  "version": "2.3.0",
  "description": "Key-Value API with pluggable backends. So simple, even Kevin Malone could use it.",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint src",
    "performance": "node benchmarks/performance.js",
    "precommit": "npm run lint",
    "prepush": "npm run dc:test",
    "dc:destroy": "docker-compose $(make dc.files) down -v --remove-orphans",
    "dc:test": "docker-compose $(make dc.files) build test && docker-compose $(make dc.files) run --rm test test --"
  },
  "keywords": [
    "key",
    "value",
    "key",
    "value",
    "database",
    "storage"
  ],
  "author": "Finbox Inc.",
  "license": "MIT",
  "devDependencies": {
    "delay": "5.0.0",
    "envvar": "2.0.0",
    "eslint": "7.19.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "5.0.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "uid": "2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/finboxio/kev"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "core.spec.js"
    ]
  },
  "dependencies": {
    "bytes": "3.1.0",
    "dataloader": "2.0.0",
    "glob-to-regexp": "0.4.1",
    "into-stream": "6.0.0",
    "ioredis": "4.19.4",
    "lru-cache": "6.0.0",
    "mongodb": "3.6.4",
    "ms": "2.1.3",
    "redis-stream": "0.1.0",
    "resurrect-js": "1.0.1",
    "stream-to-array": "2.3.0",
    "stream-transform": "2.0.4"
  }
}
