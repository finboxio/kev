{
  "name": "kev",
  "version": "1.0.1",
  "description": "Key-Value API with pluggable backends. So simple, even Kevin Malone could use it.",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint src",
    "precommit": "npm run lint",
    "prepush": "npm run dc:test",
    "dc:destroy": "docker-compose $(make dc.files) down -v --remove-orphans",
    "dc:test": "docker-compose $(make dc.files) build test && docker-compose $(make dc.files) run test test --"
  },
  "keywords": [
    "key",
    "value",
    "key",
    "value",
    "database",
    "storage"
  ],
  "author": "Finbox Inc.",
  "license": "MIT",
  "devDependencies": {
    "delay": "4.2.0",
    "envvar": "2.0.0",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jest": "22.6.4",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "husky": "2.4.0",
    "jest": "24.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/finboxio/kev"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "dataloader": "1.4.0",
    "glob-to-regexp": "0.4.1",
    "into-stream": "5.1.0",
    "ioredis": "4.9.5",
    "mongodb": "3.2.7",
    "ms": "2.1.1",
    "resurrect-js": "1.0.1",
    "stream-to-array": "2.3.0",
    "stream-transform": "2.0.0"
  }
}
